#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ewidlund/dev/bitspace/node_modules/.pnpm/ts-jest@29.1.4_@babel+core@7.24.6_jest@29.7.0_typescript@5.4.5/node_modules/ts-jest/node_modules:/Users/ewidlund/dev/bitspace/node_modules/.pnpm/ts-jest@29.1.4_@babel+core@7.24.6_jest@29.7.0_typescript@5.4.5/node_modules:/Users/ewidlund/dev/bitspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ewidlund/dev/bitspace/node_modules/.pnpm/ts-jest@29.1.4_@babel+core@7.24.6_jest@29.7.0_typescript@5.4.5/node_modules/ts-jest/node_modules:/Users/ewidlund/dev/bitspace/node_modules/.pnpm/ts-jest@29.1.4_@babel+core@7.24.6_jest@29.7.0_typescript@5.4.5/node_modules:/Users/ewidlund/dev/bitspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-jest@29.1.4_@babel+core@7.24.6_jest@29.7.0_typescript@5.4.5/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-jest@29.1.4_@babel+core@7.24.6_jest@29.7.0_typescript@5.4.5/node_modules/ts-jest/cli.js" "$@"
fi
