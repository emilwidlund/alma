{
    "name": "@usealma/client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "prisma generate && next build",
        "start": "next start",
        "lint": "next lint",
        "prisma:pull": "dotenv -e .env.local -- prisma db pull",
        "prisma:push": "dotenv -e .env.local -- prisma db push",
        "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
        "prisma:generate": "prisma generate && supabase gen types typescript --local > lib/database.types.ts",
        "prisma:studio": "dotenv -e .env.local -- prisma studio",
        "prisma:seed": "dotenv -e .env.local -- ts-node prisma/seed.ts",
        "codegen": "graphql-codegen --config codegen.ts"
    },
    "dependencies": {
        "@apollo/client": "^3.8.1",
        "@apollo/server": "^4.9.2",
        "@as-integrations/next": "^2.0.2",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@monaco-editor/react": "^4.5.1",
        "@mui/icons-material": "^5.11.16",
        "@mui/material": "^5.13.5",
        "@nodl/core": "^1.0.3",
        "@nodl/react": "^1.0.3",
        "@prisma/client": "^5.2.0",
        "@supabase/auth-helpers-nextjs": "^0.7.2",
        "@supabase/auth-helpers-react": "^0.4.0",
        "@supabase/supabase-js": "^2.26.0",
        "@usealma/renderer": "*",
        "@usealma/types": "*",
        "@usealma/webgl": "*",
        "autoprefixer": "10.4.14",
        "clsx": "^1.2.1",
        "dotenv-cli": "^7.2.1",
        "eslint": "8.42.0",
        "eslint-config-next": "13.4.4",
        "framer-motion": "^10.12.16",
        "graphql": "^15.8.0",
        "graphql-scalars": "^1.22.2",
        "graphql-tag": "^2.12.6",
        "immer": "^10.0.2",
        "lodash": "^4.17.21",
        "monaco-editor": "^0.39.0",
        "next": "13.4.4",
        "postcss": "8.4.24",
        "react": "18.2.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-dom": "18.2.0",
        "react-draggable": "^4.4.5",
        "react-query": "^3.39.3",
        "rxjs": "^7.8.1",
        "supabase": "^1.74.2",
        "use-pan-and-zoom": "^0.6.5",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "5.0.0",
        "@graphql-codegen/client-preset": "^4.1.0",
        "@graphql-codegen/introspection": "4.0.0",
        "@graphql-codegen/typescript": "^4.0.1",
        "@graphql-codegen/typescript-resolvers": "^4.0.1",
        "@types/micro-cors": "^0.1.3",
        "@types/node": "20.2.5",
        "@types/react": "18.2.8",
        "@types/react-beautiful-dnd": "^13.1.4",
        "@types/react-dom": "18.2.4",
        "tailwindcss": "3.3.2",
        "ts-node": "^10.9.1",
        "typescript": "5.1.3"
    }
}
